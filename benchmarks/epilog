
a = 0
x = 0
y = 0
while x + y < s:
    a = a + 1/4 @ 1/3; a + 1/3 @ 1/3; a + 1/2
    b = a - 100 @ 1/3; a - 90 @ 1/3; a - 110
    c = a - 400 @ 1/2; a - 600
    d = a - 990 @ 1/2; a - 1010
    e = (1/200) * b
    f = (1/100) * c
    g = (1/1000) * d
    step = b*c*d
    x = x + step
    y = y + step
